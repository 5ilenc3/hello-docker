pipeline {
  agent {
    kubernetes {
      	cloud 'kubernetes'
      	label 'hello-docker'
      	defaultContainer 'jnlp'
      }
    }
  stages {
    stage('Deploy App') {
      steps {
        script {
          kubernetesDeploy(configs: "hellodocker.yaml", kubeconfigId: "MINIKUBECONFIG")
        }
      }
    }
  }
}
